{
  "swagger": "2.0",
  "info": {
    "description": "API documentation for the Flutter Calory Tool server",
    "version": "1.0.0",
    "title": "Flutter Calory Tool API",
    "contact": {
      "email": "support@calorytool.com"
    }
  },
  "host": "localhost:3000",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "paths": {
    "/foods/search": {
      "get": {
        "tags": [
          "Foods"
        ],
        "summary": "Search foods",
        "description": "Search for foods based on query parameters",
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "description": "The search query",
            "required": true,
            "type": "string"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "schema": {
              "$ref": "#/definitions/foodSearchModel"
            }
          },
          "400": {
            "description": "Not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/recipes/search": {
      "get": {
        "tags": [
          "Recipes"
        ],
        "summary": "Search recipes",
        "description": "Search for recipes based on query parameters",
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "description": "The search query",
            "required": true,
            "type": "string"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "schema": {
              "$ref": "#/definitions/recipeSearchModel"
            }
          },
          "400": {
            "description": "Not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
