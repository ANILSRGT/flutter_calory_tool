    "foodSearchModel": {
      "type": "object",
      "properties": {
        "pageNumber": {
          "type": "integer"
        },
        "foods": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/foodModel"
          }
        }
      }
    },
    "foodModel": {
      "type": "object",
      "properties": {
        "id": {
